    "angular-ui-bootstrap": "~2.5.0",
    "angular-ui-router": "~0.4.2",
    "angular-unsavedchanges": "~0.2.5",
    "angular-utf8-base64": "0.0.5",
    "bootstrap": "~3.3.7",
