    "compression-webpack-plugin": "~1.0.1",
    "copy-webpack-plugin": "~4.1.1",
    "css-loader": "~0.28.7",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "~1.1.5",
    "fork-ts-checker-webpack-plugin": "~0.2.8",
